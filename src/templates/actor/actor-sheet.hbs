<form class="flexcol {{cssClass}} {{actor.type}}" autocomplete="off">
  {{!-- Sheet Header --}}
  {{> "systems/smt-tc/templates/parts/actor/header.hbs" }}

  {{!-- Tab Navigation --}}
  <nav class="sheet-tabs tabs" data-group="primary">
    {{!-- Default tab is defined in actor-sheet.ts --}}
    <a class="item resource-label" data-tab="main">{{localize "SMT.tabs.main"}}</a>
    {{!-- <a class="item resource-label" data-tab="inventory">{{localize "SMT.tabs.inventory"}}</a> --}}
    {{!-- <a class="item resource-label" data-tab="bio">{{localize "SMT.tabs.bio"}}</a> --}}
    <a class="item resource-label" data-tab="notes">{{localize "SMT.tabs.notes"}}</a>
  </nav>

  {{!-- Sheet Body --}}
  <section class="sheet-body">
    {{!-- Main Tab --}}
    <div class="tab main" data-group="primary" data-tab="main">
      {{> "systems/smt-tc/templates/parts/actor/tabs/main.hbs"}}
    </div>

    {{!-- Inventory Tab --}}
    {{!-- <div class="tab inventory" data-group="primary" data-tab="inventory"> --}}
      {{!-- {{> "systems/smt-tc/templates/parts/actor/tabs/inventory.hbs"}} --}}
    {{!-- </div> --}}

    {{!-- Bio Tab --}}
    {{!-- <div class="tab bio" data-group="primary" data-tab="bio"> --}}
      {{!-- {{> "systems/smt-tc/templates/parts/actor/tabs/bio.hbs"}} --}}
    {{!-- </div> --}}

    {{!-- Notes Tab --}}
    <div class="tab notes" data-group="primary" data-tab="notes">
      {{editor system.notes target="system.notes" button=true editable=editable}}
    </div>
  </section>
</form>